<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">

        <!-- Sequences for sale.order -->
        <record id="seq_store_request" model="ir.sequence">
            <field name="name">Store requisition</field>
            <field name="code">store.request</field>
            <field name="prefix">SR</field>
            <field name="padding">5</field>
            <field name="company_id" eval="False"/>
        </record>

        <record id="seq_sale_mrp" model="ir.sequence">
            <field name="name">Production from sales</field>
            <field name="code">sale.mrp</field>
            <field name="prefix">PFS</field>
            <field name="padding">5</field>
            <field name="company_id" eval="False"/>
        </record>

        <record id="seq_purchase_request" model="ir.sequence">
            <field name="name">Production Request</field>
            <field name="code">purchase.request</field>
            <field name="prefix">SM</field>
            <field name="padding">5</field>
            <field name="company_id" eval="False"/>
        </record>
    </data>
</odoo>
